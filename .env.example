# Server Configuration
PORT=3001
NODE_ENV=development

# IDX Broker API Configuration
# Get these from your IDX Broker account at https://middleware.idxbroker.com/
IDX_API_URL=https://api.idxbroker.com
IDX_API_KEY=your_idx_api_key_here
IDX_PARTNER_KEY=your_idx_partner_key_here

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Database Configuration (if needed for caching)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=real_estate_db
DB_USER=postgres
DB_PASSWORD=your_db_password

# Redis Configuration (for caching IDX responses)
REDIS_URL=redis://localhost:6379
REDIS_TTL=3600

# Logging
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here